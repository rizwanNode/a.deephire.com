{
  "name": "DeepHireNodeAPI",
  "version": "1.0.0",
  "description": "Automate the recruiting process with one-way video interviews",
  "main": "index.js",
  "scripts": {
    "prod": "node dist/index.js",
    "compile": "babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "start": "nodemon server --exec babel-node --config .nodemonrc.json | pino-pretty",
    "test": "jest --forceExit",
    "dev:debug": "nodemon server --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "test:debug": "jest --detectOpenHandles",
    "lint": "eslint -c .eslintrc.json {server,test}/**",
    "lint:fix": "eslint --fix -c .eslintrc.json {server,test}/**"
  },
  "dependencies": {
    "auth0": "2.20.0",
    "aws-sdk": "^2.403.0",
    "body-parser": "^1.18.2",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "jwks-rsa": "^1.3.0",
    "jwt-decode": "^2.2.0",
    "mandrill-api": "1.0.45",
    "mixpanel": "0.10.2",
    "mongodb": "3.1.10",
    "node-cache": "4.2.0",
    "node-fetch": "2.3.0",
    "pino": "^5.10.2",
    "swagger-express-middleware": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "7.4.4",
    "@shelf/jest-mongodb": "1.0.1",
    "babel-eslint": "^8.2.2",
    "babel-jest": "24.8.0",
    "babel-preset-env": "1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.14.0",
    "jest": "24.8.0",
    "nodemon": "^1.18.4",
    "pino-pretty": "^2.2.2",
    "superagent": "5.0.5",
    "supertest": "4.0.2"
  },
  "author": "Russell Ratcliffe <russell@deephire.com> (https://github.com/russeii)"
}
